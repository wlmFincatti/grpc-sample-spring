{
  "request": {
    "urlPathPattern": "/account/[0-9]",
    "method": "GET"
  },
  "response": {
    "headers": {
      "Content-Type": "application/json"
    },
    "status": 200,
    "jsonBody": {
      "idAccount": "{{request.path.[1]}}",
      "agency": 1234,
      "number": 154789,
      "digit": 2,
      "brand": "Another"
    },
    "transformers": [
      "response-template"
    ]
  }
}